{
  "name": "@mongosh/arg-parser",
  "version": "1.4.1",
  "description": "MongoDB Shell CLI Argument List Parser Package",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "test": "mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register \"./{src,lib}/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "lint": "eslint --report-unused-disable-directives \"./{src,test}/**/*.{js,ts,tsx}\"",
    "check": "npm run lint && depcheck --skip-missing=true",
    "compile-ts": "tsc -p tsconfig.json",
    "prepublish": "npm run compile-ts"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "dependencies": {
    "@mongosh/errors": "1.4.1",
    "@mongosh/i18n": "1.4.1",
    "mongodb-connection-string-url": "^2.5.2"
  },
  "devDependencies": {
    "@mongodb-js/devtools-connect": "^1.4.2"
  },
  "gitHead": "4e88ce238c93af5f2eb5ad9f9e3a2ac5ef1eb2e0"
}
